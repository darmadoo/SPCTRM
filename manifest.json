{
	"manifest_version": 2,

	"name": "Spctrm", 
	"description": "Gather your favorite colors in one place.",
	"version": "1.11.1", 
	"short_name": "Spctrm",
	"permissions": [
		"storage",
		"tabs",
		"activeTab",
		"https://ajax.googleapis.com/"
	],

	"icons": {"128": "img/icon.png"}, 

	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "Spcrtm",
		"default_popup": "popup.html" 
	} 
}